

{
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "manifest_version": 2,
    "name": "armadillo",
    "version": "18424.827.1",


    "description": "Google Fuchsia OS launcher for Chrome OS",
    "icons": {
        "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
    },

    "author": "Google inc.",
    "kiosk_enabled": true,
    "minimum_chrome_version": "38",
    "offline_enabled": true,
  
    "permissions": [
        "power",
        "accessibilityFeatures.read",
        "accessibilityFeatures.modify",
        "<all_urls>",
        "audio", "audioCapture", "notifications", "videoCapture", "webview", "storage", "notifications",
       "https://fonts.googleapis.com/css?family=Roboto:300,400,500,700",
  "https://code.getmdl.io/1.3.0/material.green-indigo.min.css",
  "http://google.com/"
    ],
    "sandbox": {
        "pages": [
            "scripts/sandbox-scripts.html"
        ]
    }
}
